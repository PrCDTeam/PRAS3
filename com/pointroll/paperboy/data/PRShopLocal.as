package com.paperboy.data {	import flash.utils.Dictionary;		public class PRShopLocal {		private var _categoryTreeIds:Dictionary;				private var _shopLocalXmlList:XMLList;						public function PRShopLocal() {			// constructor code		}				public function addCategoryTreeId(id:int):void{			if(!_categoryTreeIds){				_categoryTreeIds = new Dictionary();			}			_categoryTreeIds[id] = id;		}				public function getCategoryTreeId(id:int):*{			if(!_categoryTreeIds[id]){				trace("categoryTreeId doesn't exist");				return null;			}			return _categoryTreeIds[id];		}		public function setShopLocalData(xl:XMLList):void{			_shopLocalXmlList = new Dictionary();			for each (var item:XML in xl) {				for each (var attr:XML in item.attributes()) {					_productDictionary[attr.name().toString()] = attr.valueOf().toString();				}			}		}	}	}