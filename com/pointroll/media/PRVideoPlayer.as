package com.pointroll.media {	import com.pointroll.interfaces.IPointRollVideo;	import com.pointroll.api.media.IVideoDefinition;		import com.pointroll.media.PRVideo;		import flash.events.EventDispatcher;	import com.pointroll.api.events.PrVideoEvent;	import com.pointroll.api.events.PrProgressEvent;		public class PRVideoPlayer extends EventDispatcher{		var _pointrollVideo:PRVideo;		var _pointrollVideoDefinition:IVideoDefinition;				var _isVideoComplete:Boolean;				public function PRVideoPlayer(pointrollVideo:PRVideo,pointrollVideoDefinition:IVideoDefinition) {			_pointrollVideo = pointrollVideo;			_pointrollVideoDefinition = pointrollVideoDefinition;		}		public function playVideo():void{			_isVideoComplete = false;			_pointrollVideo.startPointRollVideo(_pointrollVideoDefinition);		}		public function resume():void{			_pointrollVideo.resume();		}		public function stop():void{			_pointrollVideo.stop();		}		public function pause():void{			_pointrollVideo.pause();		}		public function restart():void{			_pointrollVideo.restart();		}		public function mute():void{			_pointrollVideo.mute();		}		public function unmute():void{			_pointrollVideo.unmute();		}		public function restartWithSound(vol:Number = 1):void{			_pointrollVideo.restartWithSound(vol);		}		public function destroy():void{			_pointrollVideo.destroy();		}		public function seek(timeInSeconds:Number):void{			_pointrollVideo.seek(timeInSeconds);		}		public function addNetStreamClient(client:Object):void{			_pointrollVideo.addNetStreamClient(client);		}		public function setVideoProperty(p:String, v:*){			_pointrollVideo.setVideoProperty(p,v);		}	}	}