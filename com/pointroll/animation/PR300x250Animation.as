package com.pointroll.animation{	import com.pointroll.abstract.PRAnimationAbstract;	import com.greensock.TweenLite;	import flash.display.MovieClip;	import flash.display.DisplayObjectContainer;	import flash.utils.Dictionary;	public class PR300x250Animation extends PRAnimationAbstract {		public function PR300x250Animation(timeline:DisplayObjectContainer) {			super(timeline);		}		override public function startAnimation():void {			TweenLite.to(_dictionary["kitchen"],_duration,{alpha:0,delay:3});			TweenLite.to(_dictionary["smallTag"],_duration,{alpha:0,delay:3});			TweenLite.to(_dictionary["gradientBG"],_duration,{alpha:1,delay:3});			TweenLite.to(_dictionary["bigTag"],_duration,{alpha:1,delay:3});			TweenLite.to(_dictionary["stove"],_duration,{alpha:1,delay:3, onComplete:onFinalAnimation});		}		// EVENTS		private function onFinalAnimation():void {			TweenLite.to(_dictionary["stove"],_duration,{alpha:0,delay:3});			TweenLite.to(_dictionary["oven"],_duration,{alpha:1,delay:3});		}	}}