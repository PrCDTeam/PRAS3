package com.pointroll.animation{	import com.pointroll.abstract.PRAnimationAbstract;		import com.greensock.TweenLite;	import flash.display.MovieClip;	import flash.display.DisplayObjectContainer;	import flash.utils.Dictionary;		public class PR160x600Animation extends PRAnimationAbstract{		public function PR160x600Animation(timeline:DisplayObjectContainer) {			super(timeline);		}		override public function startAnimation():void {			TweenLite.to(_dictionary["stove"],_duration,{alpha:0,delay:3, onComplete:onFinalAnimation});		}		// EVENTS		private function onFinalAnimation():void {			TweenLite.to(_dictionary["oven"],_duration,{alpha:1});		}	}}