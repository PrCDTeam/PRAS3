package com.pointroll.animation{	import com.pointroll.abstract.PRAnimationAbstract;	import com.greensock.TweenLite;	import flash.display.MovieClip;	import flash.display.DisplayObjectContainer;	import flash.utils.Dictionary;	public class PR728x90Animation extends PRAnimationAbstract {		public function PR728x90Animation(timeline:DisplayObjectContainer) {			super(timeline);		}		override public function startAnimation():void {			TweenLite.to(_dictionary["copy1"],_duration,{alpha:0,delay:4, onComplete:onFinalAnimation});		}		// EVENTS		private function onFinalAnimation():void {			TweenLite.to(_dictionary["copy2"],_duration,{alpha:1});		}	}}