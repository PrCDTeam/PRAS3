package com.pointroll.abstract {		import flash.display.Sprite;	import flash.display.Stage;	import flash.display.DisplayObjectContainer;		import flash.events.EventDispatcher;	import com.pointroll.utils.PRScroller;	import com.pointroll.utils.PointRollUtils;	import com.pointroll.paperboy.ProductBuilder;	import flash.errors.IllegalOperationError;		public class PointRollAbstractUI extends EventDispatcher{		private var _timeline:DisplayObjectContainer;		public var _productBuilder:ProductBuilder;		public var _scroller:PRScroller;		private var _clickTag:Sprite;		public var _logo:Logo;				public function PointRollAbstractUI(timeline:DisplayObjectContainer) {			_timeline = timeline;			init();		}		public function init():void{						throw new IllegalOperationError("Abstract method: must be overridden in a subclass");			return null;		}				public function buildUI(...args):void{						throw new IllegalOperationError("Abstract method: must be overridden in a subclass");			return null;		}		public function update(...args):void{			throw new IllegalOperationError("Abstract method: must be overridden in a subclass");			return null;					}				// GETTERS		public function getUIWidth():Number{			return _timeline.stage.stageWidth;		}		public function getUIHeight():Number{			return _timeline.stage.stageHeight;		}				// Print UI 		public function printUI():String{			return "UI SIZE: " + getUIWidth() + "x" +  getUIHeight();		}	}	}