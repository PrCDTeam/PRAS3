package com.pointroll.abstract {	import flash.display.Stage;	import flash.display.DisplayObjectContainer;		import flash.events.EventDispatcher;		import flash.errors.IllegalOperationError;		import flash.utils.Dictionary;		public class PRAnimationAbstract extends EventDispatcher{		private var _timeline:DisplayObjectContainer;				public var _dictionary:Dictionary;				public var _duration:Number;				public function PRAnimationAbstract(timeline:DisplayObjectContainer) {			_timeline = timeline;			_duration = .5;			_dictionary = new Dictionary();					}		public function startAnimation():void{			throw new IllegalOperationError("Abstract method: must be overridden in a subclass");			return null;		}		public function addAnimationObject(objectName:String, objectValue:*):void{			_dictionary[objectName] = objectValue;		}				// SETTERS		public function set duration(v:Number):void{			_duration = v;		}			}	}