package com.pointroll.factory {	import com.pointroll.interfaces.ITradePage;		import com.pointroll.factory.ViewTradePage;	import com.pointroll.factory.AcceptTradePage;	import com.pointroll.factory.EvaluateTradePage;	import com.pointroll.factory.ProposeTradePage;	import com.pointroll.factory.ProtestTradePage;	import com.pointroll.factory.TransactionTradePage;	import com.pointroll.factory.DefaultTradePage;		public class SimpleFactory {		public static function createTradePage(placementID:Number):ITradePage{						switch(placementID){				// View Trade Page:2118904, 2118910, 2118916				case 2118904:				case 2118910:				case 2118916:				case 2123978:				case 2123979:				case 2123980:					 trace("createTradepage("+placementID+")"+" return ViewTradePage()");					 return new ViewTradePage();				break;				// Accept Trade Page 2118905, 2118911, 2118917				case 2118905:				case 2118911:				case 2118917:				case 2123981:				case 2123982:					 trace("createTradepage("+placementID+")"+" return AcceptTradePage()");					 return new AcceptTradePage();				break;				// Evaluate Trade Page 2118906, 2118912, 2118918				case 2118906:				case 2118912:				case 2118918:				case 2123983:				case 2123984:				case 2123985:				case 2123986:					 trace("createTradepage("+placementID+")"+" return EvaluateTradePage()");					 return new EvaluateTradePage();				break;				// Propose Trade Page 2118907, 2118913, 2118919				case 2118907:				case 2118913:				case 2118919:				case 2123987:				case 2123988:				case 2123989:				case 2123990:					 trace("createTradepage("+placementID+")"+" return ProposeTradePage()");					 return new ProposeTradePage();				break;				// Protest Trade Page 2118908, 2118914, 2118920				case 2118908:				case 2118914:				case 2118920:				case 2123991:				case 2123992:					 trace("createTradepage("+placementID+")"+" return ProtestTradePage()");					 return new ProtestTradePage();				break;				// Transaction Page 2118909, 2118915, 2118921				case 2118909:				case 2118915:				case 2118921:				case 2123993:				case 2123994:				case 2123995:				 	 trace("createTradepage("+placementID+")"+" return TransactionTradePage()");					 return new TransactionTradePage();				break;				default:					 trace("Not a valid Placement ID. Defaulting. return DefaultTradePage()");					 return new DefaultTradePage();				break;			}			return new DefaultTradePage();		}	}	}