package com.pointroll.factory {	import com.pointroll.abstract.PRAnimationAbstract;		import com.pointroll.animation.PR160x600Animation;	import com.pointroll.animation.PR300x250Animation;	import com.pointroll.animation.PR728x90Animation;	import flash.display.DisplayObjectContainer;			public class PRBannerAnimationFactory {		public static function createAnimationPage(StageSize:int, timeline:DisplayObjectContainer):PRAnimationAbstract{						switch(StageSize){								case 160:					 return new PR160x600Animation(timeline);				break;				case 300:					 return new PR300x250Animation(timeline);				break;				case 728:					 return new PR728x90Animation(timeline);				break;				default:					 trace("PR:> UI size not found");				break;			}			return null;//PointRollPanel728UI(timeline);		}	}	}