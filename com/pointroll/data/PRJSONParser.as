package com.pointroll.data{	import com.pointroll.interfaces.IPRDataParser;		import flash.display.Sprite;	import flash.events.Event;	import flash.events.EventDispatcher;		import com.pointroll.events.PRParserEvents;		import com.maccherone.json.JSON;		public class PRJSONParser extends EventDispatcher implements IPRDataParser{		private var _jObj:Object;		public function PRJSONParser() {		}		public function parseData(sourceData:*,...args):* {			_jObj = JSON.decode(sourceData);			if(_jObj){				dispatchEvent(new PRParserEvents(PRParserEvents.ON_PARSER_COMPLETE,obj));			}			else{				dispatchEvent(new PRParserEvents(PRParserEvents.ON_PARSER_FAIL,obj));			}		}		public function getParseData():* {			return _jObj;		}	}}