package com.pointroll.data {	    import com.pointroll.api.data.AdControl;    import com.pointroll.api.events.AdControlEvent;		import flash.display.Sprite;	import flash.display.MovieClip;	public class PRAdControl{		private var _adControl:AdControl				public function PRAdControl() {			_adControl = new AdControl();		}		public function load():void{			_adControl.load();		}				public function changeZip(zip:String,autoLoad:Boolean=true):void{			_adControl.changeZip(zip,autoLoad);					}		public function clearSTSData(name:String):void{			_adControl.clearSTSData(name);		}		public function addAdControlEvent(eventName:String, callBack:Function):void{			_adControl.addEventListener(eventName, callBack);		}						// SETTERS & GETTERS		public function getXMLData():XML{			return _adControl.getXMLData();					}				public function set localTestFile(file:String):void{			_adControl.localTestFile = file;					}		public function set interfaceID(value:String):void{			_adControl.interfaceID = value;		}				public function get AdvInputData():Object{			return _adControl.AdvInputData;					}		public function get BTData():Object{			return _adControl.BTData;		}				public function get destURL():String{			return _adControl.destURL;		}	}}